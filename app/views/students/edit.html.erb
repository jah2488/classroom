<%= link_to 'Back', url_for(:back), class: 'btn btn-primary' %>
<h1><%= student.name %></h1>

<p class='muted'>Contact Info</p>
<section>
  <%= simple_form_for(student) do |f| %>
    <div class='form-inputs'>
      <%= f.input :name %>
      <%= f.input :github, hint: 'Not the full url just your profile name' %>
      <%= f.input :phone %>
      <%= f.input :twitter, hint: 'Not the full url just your profile name' %>
      <%= f.input :blog, as: :url %>
      <%= f.input :bio, as: :text %>
    </div>
    <div class='form-actions'>
      <%= f.button :submit %>
    </div>
  <% end %>
  <ul>
  </ul>
</section>
